<Tab name="Chams">
    <TabContainer width="fill" height="fill">
        <Tab name="Chams" padding="4 4 4 4">
            <Box padding="12 6 6 6" width="content" height="content" name="Chams">
                <List width="200">
                    <AutoVariable width="fill" target="chams.enable" label="Entity Chams"/>
                    <AutoVariable width="fill" target="chams.legit" label="Legit" tooltip="Don't show chams through walls."/>
                    <AutoVariable width="fill" target="chams.original" label="Draw original model"/>
                    <AutoVariable width="fill" target="chams.type" label="Chams Type" tooltip="0 = Normal, 1 = Flat, 2 = Shaded, 3 = Glossy, 4 = Glow, 5 = Wireframe"/>
                </List>
            </Box>
            <Box padding="12 6 6 6" width="content" height="content" name="Weapons" y="85">
                <List width="200">
                    <AutoVariable width="fill" target="chams.weapons.enemy" label="Enemy Weapon Chams"/>
                    <AutoVariable width="fill" target="chams.weapons.teammate" label="Teammate Weapon Chams"/>
                    <AutoVariable width="fill" target="chams.weapons.original" label="Show Original Weapons"/>
                </List>
            </Box>
            <Box padding="12 6 6 6" width="content" height="content" name="Material" y="170">
                <List width="200">
                    <AutoVariable width="fill" target="chams.phong" label="Enable Phong"/>
                    <AutoVariable width="fill" target="chams.phongboost" label="Phong Boost"/>
                    <AutoVariable width="fill" target="chams.envmaptint" label="Envmap Tint" tooltip="0-1, Higher = more shiny"/>
                    <AutoVariable width="fill" target="chams.rimlight" label="Rimlight" tooltip="0-1, Edge lighting"/>
                </List>
            </Box>
            <Box padding="12 6 6 6" width="content" height="content" name="Colors" y="255">
                <List width="200">
                    <AutoVariable width="fill" target="chams.red" label="Red team visible"/>
                    <AutoVariable width="fill" target="chams.blu" label="Blu team visible"/>
                    <AutoVariable width="fill" target="chams.novis.red" label="Red team through walls"/>
                    <AutoVariable width="fill" target="chams.novis.blu" label="Blu team through walls"/>
                </List>
            </Box>
            <Box padding="12 6 6 6" width="content" height="content" name="Targets" x="210">
                <List width="150">
                    <AutoVariable width="fill" target="chams.show.players" label="Players"/>
                    <AutoVariable width="fill" target="chams.show.teammates" label="Teammates"/>
                    <AutoVariable width="fill" target="chams.self" label="Local player"/>
                    <AutoVariable width="fill" target="chams.show.buildings" label="Buildings"/>
                    <AutoVariable width="fill" target="remove.arms" label="Remove arms"/>
                </List>
            </Box>
            <Box padding="12 6 6 6" width="content" height="content" name="Alpha" x="370">
                <List width="150">
                    <AutoVariable width="fill" target="chams.player.alpha" label="Player Alpha"/>
                    <AutoVariable width="fill" target="chams.building.alpha" label="Building Alpha"/>
                </List>
            </Box>
        </Tab>
    </TabContainer>
</Tab>
